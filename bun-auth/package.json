{
  "name": "bun-auth",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "db:generate": "bun --env-file .env --bun drizzle-kit generate",
    "db:migrate": "bun --env-file .env --bun drizzle-kit migrate",
    "db:studio": "bun --env-file .env --bun drizzle-kit studio",
    "db:push": "bun --env-file .env --bun drizzle-kit push",
    "example": "bun run example-usage.ts"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.1.2",
    "@elysiajs/openapi": "^1.4.11",
    "better-auth": "^1.4.5",
    "drizzle-orm": "^0.45.0",
    "elysia": "1.4.18",
    "pg": "^8.16.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/pg": "^8.15.6",
    "bun-types": "1.3.3",
    "drizzle-kit": "^0.31.8"
  },
  "module": "src/index.js"
}
